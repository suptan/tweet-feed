{
  "name": "tweet-feed",
  "version": "0.0.1",
  "description": "Frontend build by NextJS with TypeScript and Redux",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --notify --config=jest.config.js",
    "test:watch": "NODE_ENV=test jest --watchAll --config=jest.config.js",
    "start": "NODE_ENV=production PORT=$PORT node ./.next/production-server/index.js",
    "build": "rm -rf .next && next build && tsc --project tsconfig.server.json",
    "dev": "nodemon server/index.ts",
    "export": "npm run build && next export",
    "prettier": "prettier  --write **/*.{ts,tsx,js}",
    "lint:ts": "npm run prettier && tslint -c tslint.json -t stylish --fix {pages,src,server}/**/*.{ts,tsx}",
    "lint:scss": "stylelint --fix {src,pages}/**/*.scss",
    "lint:types": "tsc -p . --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint:types"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run lint:ts",
      "git add"
    ],
    "*.scss": [
      "npm run lint:scss",
      "git add"
    ]
  },
  "author": "Sup Tan",
  "repository": {
    "type": "git",
    "url": "git@github.com:suptan/tweet-feed.git"
  },
  "license": "ISC",
  "dependencies": {
    "@zeit/next-sass": "1.0.1",
    "antd": "^4.3.3",
    "axios": "0.19.2",
    "dotenv": "8.2.0",
    "dotenv-webpack": "^1.8.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "lru-cache": "^5.1.1",
    "next": "9.3.2",
    "next-compose-plugins": "2.2.0",
    "next-redux-wrapper": "5.0.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "redux": "4.0.5",
    "redux-devtools-extension": "2.13.8",
    "redux-thunk": "2.3.0",
    "svg-sprite-loader": "4.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.2.1",
    "@types/express": "^4.17.6",
    "@types/jest": "25.1.4",
    "@types/lodash": "^4.14.155",
    "@types/lru-cache": "^5.1.0",
    "@types/next-redux-wrapper": "3.0.0",
    "@types/node": "13.9.5",
    "@types/react": "16.9.27",
    "@types/react-redux": "7.1.7",
    "@types/svg-sprite-loader": "3.9.1",
    "acorn": "7.1.1",
    "babel-core": "6.26.3",
    "babel-jest": "25.2.4",
    "babel-loader": "8.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "husky": "4.2.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "25.2.4",
    "jest-svg-transformer": "1.0.0",
    "lint-staged": "10.1.0",
    "node-sass": "4.9.4",
    "nodemon": "2.0.2",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.0.2",
    "redux-mock-store": "^1.5.4",
    "stylelint": "13.2.1",
    "stylelint-config-prettier": "8.0.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-scss": "3.16.0",
    "svgo": "1.3.2",
    "svgo-loader": "2.2.1",
    "ts-jest": "^26.1.0",
    "ts-node": "8.8.1",
    "tslint": "6.1.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "4.2.0",
    "typescript": "3.8.3"
  }
}
